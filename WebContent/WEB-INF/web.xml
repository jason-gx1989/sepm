<?xml version="1.0" encoding="UTF-8"?>

<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
 http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<welcome-file-list>
		<welcome-file>test.jsp</welcome-file>
	</welcome-file-list>
	
	
	<servlet>
		<servlet-name>addShiftManager</servlet-name>
		<servlet-class>addshiftManager.AddShiftManagerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addShiftManager</servlet-name>
		<url-pattern>/addShiftManager</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>addStaff</servlet-name>
		<servlet-class>addstaff.AddStaffServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addStaff</servlet-name>
		<url-pattern>/addStaff</url-pattern>
	</servlet-mapping>
	


</web-app>
